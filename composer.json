{
    "name": "manuxi/sulu-content-types-bundle",
    "type": "sulu-bundle",
    "description": "Custom Content Types for Sulu CMS: NumberWithDefault, ColorSelect, SliderRange",
    "keywords": [
        "sulu",
        "content-type",
        "cms",
        "bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Manuxi",
            "homepage": "https://github.com/manuxi"
        }
    ],
    "require": {
        "php": "^8.1",
        "sulu/sulu": "^2.6",
        "symfony/config": "^5.4 || ^6.0 || ^7.0",
        "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
        "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "jackalope/jackalope-doctrine-dbal": "^1.3.4",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Manuxi\\SuluContentTypesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manuxi\\SuluContentTypesBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
